{
  "name": "redxt",
  "version": "1.0.0",
  "description": "Cloudflare Worker backend for AI agent browser automation",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "esbuild src/index.ts --bundle --format=esm --outfile=dist/index.js --platform=neutral --conditions=worker",
    "tail": "wrangler tail",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cloudflare-workers",
    "durable-objects",
    "ai-agent",
    "browser-automation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@cloudflare/vitest-pool-workers": "^0.9.12",
    "@cloudflare/workers-types": "^4.20251008.0",
    "@types/node": "^24.7.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "3.2.4",
    "wrangler": "^4.42.2"
  },
  "dependencies": {
    "itty-router": "^5.0.0",
    "@anthropic-ai/sdk": "^0.20.0"
  }
}
